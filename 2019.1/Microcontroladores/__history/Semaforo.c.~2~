//#include <Semaforo.h>

#include <16F877A.h>
#device ADC=10

#FUSES NOLVP                    //No low voltage prgming, B3(PIC16) or B5(PIC18) used for I/O

#use delay(crystal=20000000)


void main()
{

   while(TRUE)
   {
   output_low(PIN_B1);
   delay_ms(100);
   output_high(PIN_B1);
   delay_ms(100);
   
      //TODO: User Code
   }

}
